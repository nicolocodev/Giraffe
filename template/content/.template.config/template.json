{
  "identity": "Giraffe.Template",
  "author": "David Sinclair and contributors",
  "classifications": [ "Web", "Giraffe" ],
  "name": "Giraffe Web App",
  "tags": {
    "language": "F#"
  },
  "shortName": "giraffe",
  "sourceName": "_AppName",
  "symbols": {
    "enablepaket": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    }  
  },
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(!enablepaket)",
          "exclude": [ "paket.references" ]
        }
      ]
    }
  ], 
  "SpecialCustomOperations": {
    "**/*.fsproj": {
      "flagPrefix": "<!--",
      "operations": [
        {
          "type": "conditional",
          "configuration": {
            "if": "if",
            "else": "else",
            "elseif": "elseif",
            "endif": "endif",
            "evaluator": "C++"
          }
        }
      ]
    }
  }
}